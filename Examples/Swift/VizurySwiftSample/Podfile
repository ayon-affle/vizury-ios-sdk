# platform :ios, '9.0'

target 'VizurySwiftSample' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VizurySwiftSample
    pod 'VizuryEventLogger', :git => 'https://github.com/ayon-affle/vizury-ios-sdk.git', :tag => 'vizuryeventlogger-1.1.9'
    pod 'Firebase/Messaging'
    pod 'Firebase/Analytics'
end


