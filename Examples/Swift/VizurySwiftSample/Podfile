  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VizurySwiftSample
    pod 'VizuryRichNotification', :git => 'https://github.com/ayon-affle/vizury-ios-sdk.git', :tag => 'richpush-1.1.2'

end
